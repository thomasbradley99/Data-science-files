import math
import matplotlib pyplot as plt


def set_initial(v_initial, theta):
    vx = v_initial * math.cos(theta)
    vy = v_initial * math.sin(theta)
    x = 0
    y = 0
    return x,y,vx,vy

def acceleration(vx, vy, beta):
    ax = -(beta)*vx**2
    ay = -(beta)*vy**2 - 9.81 
    return ax, ay

